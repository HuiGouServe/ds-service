<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pingan.Mapper.UserMapper" >
    <select id="selectUserByAccountAndPassword" parameterType="String" resultType="com.pingan.Object.User">
        select * from `user` where user_account = #{userAccount} and password = #{password}
    </select>
</mapper>

